macro_rules! C {
    ($class0:tt, $($class:tt),+) => {
        [$class0, $(C!($class),)*]
    };
    ("flex") => {
        "flex"
    };
    ("md:hover:bg-blue") => {
        "md:hover:bg-blue"
    };
    ("-mx-6") => {
        "-mx-6"
    };
    ("static") => {
        "static"
    };
    ("w-3/5") => {
        "w-3/5"
    };
    ("container") => {
        "container"
    };
    ("uk-child-width-1-1@xl") => {
        "uk-child-width-1-1@xl"
    };
    ("fa") => {
        "fa"
    };
    ("fas") => {
        "fas"
    };
}